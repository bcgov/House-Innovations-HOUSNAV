{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,css}\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:open": "cypress open"
  },
  "dependencies": {
    "@repo/ui": "*",
    "axe-core": "^4.9.1",
    "cypress-axe": "^1.5.0",
    "next": "^14.1.1",
    "react": "^18.2.0",
    "react-aria-components": "^1.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "@bcgov/bc-sans": "^2.1.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.1",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "cypress": "^13.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.2.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
